    "def ChangeString(s1):\n",
    "    st2=\"\"\n",
    "    for i in s1:\n",
    "        x=chr(ord(i)+1)\n",
    "        if x.lower() in ('a','e','i','o','u'):\n",
    "           x=x.upper()\n",
    "        st2+=x\n",
    "    print(st2)"
   ]
